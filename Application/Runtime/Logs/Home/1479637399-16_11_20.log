&nbsp;&nbsp;&nbsp;&nbsp;android:orientation=&quot;vertical&quot;&nbsp;&nbsp;
&nbsp;&nbsp;&nbsp;&nbsp;android:layout_width=&quot;match_parent&quot;&nbsp;&nbsp;
&nbsp;&nbsp;&nbsp;&nbsp;android:layout_height=&quot;match_parent&quot;&gt;&nbsp;&nbsp;
&nbsp;&nbsp;&nbsp;&nbsp;&lt;LinearLayout&nbsp;android:layout_width=&quot;match_parent&quot;&nbsp;&nbsp;&nbsp;
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;android:id=&quot;@+id/linearLayout1&quot;&nbsp;&nbsp;&nbsp;&nbsp;
