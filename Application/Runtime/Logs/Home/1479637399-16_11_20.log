&nbsp;&nbsp;&nbsp;&nbsp;decryptor.Put((byte&nbsp;*)cipherText,&nbsp;strlen(cipherText));
&nbsp;&nbsp;&nbsp;&nbsp;decryptor.MessageEnd();&nbsp;&nbsp;&nbsp;&nbsp;return&nbsp;outstr;
}std::string&nbsp;CTR_AESEncryptStr(std::string&nbsp;sKey,&nbsp;std::string&nbsp;sIV,&nbsp;const&nbsp;char&nbsp;*plainText)
{&nbsp;&nbsp;&nbsp;&nbsp;std::string&nbsp;outstr;&nbsp;&nbsp;&nbsp;&nbsp;//填key&nbsp;&nbsp;&nbsp;&nbsp;
